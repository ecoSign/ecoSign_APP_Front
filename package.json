{
  "name": "ecosignapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 13'",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "format": "prettier --check ./src"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-top-tabs": "^6.5.1",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^1.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cz-conventional-emoji": "^1.0.2",
    "dayjs": "^1.11.6",
    "react": "18.1.0",
    "react-hook-form": "^7.40.0",
    "react-native": "0.70.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-pager-view": "^6.1.2",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^3.3.4",
    "react-native-vector-icons": "^9.2.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-native-linear-gradient": "^2.4.0",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components-react-native": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.5",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-native": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.8.0",
    "react-native-flipper": "^0.146.1",
    "react-test-renderer": "17.0.2",
    "redux-flipper": "^2.0.1",
    "typescript": "*"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "yarn lint"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    },
    "cz-customizable": {
      "config": "cz-config.js"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
